@model IEnumerable<SelKirimDokumSite.Models.FaqItem>
<div class="accordion" id="faq">
@{
    var i = 0;
}
@foreach (var f in Model)
{
    var id = $"faq_{i++}";
    <div class="accordion-item">
        <button class="accordion-toggle" type="button" data-target="#@id">@f.Q</button>
        <div class="accordion-content" id="@id">
            <p>@f.A</p>
        </div>
    </div>
}
</div>